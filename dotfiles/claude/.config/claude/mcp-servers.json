{
  "nixos": {
    "type": "stdio",
    "command": "uvx",
    "args": [
      "mcp-nixos"
    ],
    "env": {}
  },
  "obsidian": {
    "type": "stdio",
    "command": "npx",
    "args": [
      "-y",
      "mcp-obsidian",
      "$HOME/Sync/notes"
    ],
    "env": {}
  },
  "tmux": {
    "type": "stdio",
    "command": "npx",
    "args": [
      "-y",
      "tmux-mcp",
      "--shell-type=zsh"
    ],
    "env": {}
  },
  "sequential-thinking": {
    "type": "stdio",
    "command": "npx",
    "args": [
      "-y",
      "@modelcontextprotocol/server-sequential-thinking"
    ],
    "env": {}
  },
  "neovim": {
    "type": "stdio",
    "command": "npx",
    "args": [
      "-y",
      "mcp-neovim-server"
    ],
    "env": {
      "NVIM_SOCKET_PATH": ".local/share/nvim/socket"
    }
  }
}
